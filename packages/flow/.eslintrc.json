{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "extends": [
    "plugin:flowtype/recommended",
    "@bannerets/eslint-config"
  ],
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "no-undef": 0,
    "no-unreachable": 0,
    "import/no-duplicates": 0
  }
}
